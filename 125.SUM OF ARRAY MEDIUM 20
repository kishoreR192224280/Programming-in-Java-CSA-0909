package SCANNER;
import java.util.Scanner;
public class SUMOFNATURAL {
	public static void main(String args[])
	{
		int n,i;
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the number");
		n=scan.nextInt();
		int sum=0;
		int array[]=new int[n];
		for(i=0;i<n;i++)
		{
			array[i]=scan.nextInt();
		}
		for(i=0;i<n;i++)
		{
			sum+=array[i];
		}
		System.out.println("sum = "+sum);
	}

}
