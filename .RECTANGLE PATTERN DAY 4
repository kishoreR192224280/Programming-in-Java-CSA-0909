package JAVA;
import java.util.Scanner;
public class RECTANGLE {
	public static void main(String args[])
	{
		int i,j,rows,columns;
		Scanner scan=new Scanner(System.in);
		System.out.print("rows and columns");
		rows=scan.nextInt();
		columns=scan.nextInt();
		for(i=1;i<=rows;i++)
		{
			for(j=1;j<=columns;j++)
			{
				System.out.print("* ");	
			}
			System.out.println();		}
	}

}
