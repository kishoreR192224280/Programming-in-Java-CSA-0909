import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Get input from the user for the first number
        System.out.println("Enter the first number for multiplication table:");
        int number1 = scanner.nextInt();

        // Get input from the user for the second number
        System.out.println("Enter the second number for multiplication table:");
        int number2 = scanner.nextInt();

        // Display multiplication table for number1
        System.out.println("Multiplication Table for " + number1 + ":");
        for (int i = 1; i <= 10; i++) {
            System.out.println(number1 + " X " + i + " = " + (number1 * i));
        }

        // Add a line break for better readability
        System.out.println();

        // Display multiplication table for number2
        System.out.println("Multiplication Table for " + number2 + ":");
        for (int i = 1; i <= 10; i++) {
            System.out.println(number2 + " X " + i + " = " + (number2 * i));
        }

        // Close the scanner
        scanner.close();
    }
}
